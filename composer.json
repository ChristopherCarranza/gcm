{
    "name": "fruitcake/laravel-gcm-notification-channel",
    "description": "Android GCM Push Notification Channel",
    "homepage": "https://github.com/fruitcake/laravel-gcm-notification-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Fruitcake",
            "email": "info@fruitcake.nl",
            "homepage": "https://fruitcake.nl"
        }
    ],
    "require": {
        "php": ">=5.5",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Fruitcake\\NotificationChannels\\Gcm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fruitcake\\NotificationChannels\\Gcm\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
